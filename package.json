{
  "name": "junte-ui-app",
  "version": "0.0.254",
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "gulp:styles": "gulp build --gulpfile ./projects/junte-ui/src/scripts/styles.ts",
    "gulp:styles:w": "gulp build --gulpfile ./projects/junte-ui/src/scripts/styles.ts --watch",
    "gulp:iconfont": "gulp build --gulpfile ./projects/junte-ui/src/scripts/iconfont/iconfont.ts --cwd ./projects/junte-ui",
    "iconfont": "gulp build --gulpfile ./projects/junte-ui/src/scripts/iconfont/iconfont.ts --cwd .",
    "clean": "rimraf ./dist",
    "serve": "wait-on -i 3000 dist/junte-ui/public_api.d.ts && npm run copy:files && ng serve --hmr",
    "start": "npm run clean && run-p *:watch serve",
    "build": "npm run iconfont && ng build --prod",
    "assets:create:folders": "mkdir ./dist/junte-ui/lib/assets/ && mkdir ./dist/junte-ui/lib/assets/icons && mkdir ./dist/junte-ui/lib/assets/fonts && mkdir ./dist/junte-ui/lib/assets/styles",
    "assets:copy:folders": "cp -r ./projects/junte-ui/src/lib/assets/fonts/ ./dist/junte-ui/lib/assets/fonts/ && cp -r projects/junte-ui/src/lib/assets/icons/animated ./dist/junte-ui/lib/assets/icons/animated && cp -r projects/junte-ui/src/lib/assets/icons/svg ./dist/junte-ui/lib/assets/icons/svg",
    "assets:copy:styles": "cp -r projects/junte-ui/src/lib/assets/styles/ ./dist/junte-ui/lib/assets/styles/",
    "copy:license": "cp ./projects/junte-ui/README.md ./dist/junte-ui",
    "copy:readme": "cp ./projects/junte-ui/LICENSE ./dist/junte-ui",
    "copy:assets": "rm -rf dist/junte-ui/lib/assets && npm run assets:create:folders && npm run assets:copy:folders && npm run assets:copy:styles",
    "copy:files": "npm run copy:license && npm run copy:readme && npm run gulp:styles && npm run copy:assets && npm run copy:iconfont",
    "copy:iconfont": "cd ./projects/junte-ui/src/scripts/iconfont && tsc  && cp iconfont.js template ../../../../../dist/junte-ui",
    "encapsulation": "gulp build --gulpfile node_modules/junte-encapsuler/encapsulation.js --cwd .",
    "encapsulation:watch": "gulp build --gulpfile node_modules/junte-encapsuler/encapsulation.js --cwd . --watch",
    "themes:watch": "gulp themes:watch",
    "themes": "gulp themes",
    "build:watch": "npm run gulp:iconfont && ng build junte-ui --watch",
    "styles:watch": "wait-on -i 3000 dist/junte-ui/public_api.d.ts && npm run gulp:styles:w",
    "build:lib": "gulp themes && npm run gulp:iconfont && ng build junte-ui",
    "build:prod": "npm run encapsulation && npm run build:lib && npm run copy:files",
    "npm:pack": "cd dist/junte-ui && npm pack",
    "package": "npm run build:prod && npm run npm:pack",
    "publish": "npm publish ./dist/junte-ui/junte-ui-$npm_package_version.tgz",
    "clean:package": "npm cache clean --force && rm -rf -- node_modules doc coverage dist dll package-lock.json",
    "refresh": "npm run clean:package && npm install",
    "processes": "lsof -Pnl +M -i -cmd | grep -E 'LISTEN|TCP|UDP'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@ngx-prism/core": "2.0.1",
    "@types/prismjs": "1.16.0",
    "core-js": "^3.2.1",
    "date-fns": "1.30.1",
    "fs-extra": "8.1.0",
    "gulp-sass": "^4.0.2",
    "junte-encapsuler": "0.0.31",
    "lodash-es": "4.17.15",
    "map-stream": "0.0.7",
    "minimist": "1.2.0",
    "node-html-parser": "1.1.16",
    "prismjs": "1.17.1",
    "query-ast": "1.0.2",
    "rxjs": "~6.5.3",
    "scss-parser": "1.0.3",
    "tslib": "1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.4",
    "change-case": "~3.1.0",
    "codelyzer": "~5.1.0",
    "del": "~5.1.0",
    "gulp-clean": "^0.4.0",
    "gulp-hash": "^4.2.2",
    "gulp-iconfont": "^10.0.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "ng-packagr": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "systemjs": "~6.0.0",
    "ts-node": "~8.3.0",
    "tsickle": "0.37.0",
    "tslint": "~5.19.0",
    "typescript": "3.5.3",
    "wait-on": "^3.2.0"
  }
}
